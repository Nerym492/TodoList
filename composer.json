{
    "name": "saro0h/to-do-list",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\AppBundle\\": "tests/AppBundle",
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "*",
        "doctrine/cache": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "symfony/asset": "6.3.*",
        "symfony/console": "*",
        "symfony/dotenv": "6.3.*",
        "symfony/flex": "*",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/security-bundle": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "6.3.*",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.39",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
